{
    "workbench.iconTheme": "vscode-icons",
    "[php]": {
        "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
    },
    "files.defaultLanguage": "plaintext",
    "breadcrumbs.enabled": true,
    "yaml.schemaStore.enable": false,
    "debug.inlineValues": "on",
    "debug.internalConsoleOptions": "openOnSessionStart",
    "debug.openDebug": "openOnSessionStart",
    "debug.toolBarLocation": "docked",
    "vsicons.dontShowNewVersionMessage": true,
    "files.eol": "\n",
    "files.trimTrailingWhitespace": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.tabSize": 4,
    "editor.linkedEditing": true,
    "editor.guides.bracketPairs": true,
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "all",
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "prettier.printWidth": 120,
    "prettier.tabWidth": 4,
    "prettier.singleQuote": true,
    "prettier.jsxSingleQuote": true,
    "prettier.trailingComma": "none",
    "prettier.vueIndentScriptAndStyle": true,
    "prettier.endOfLine": "auto",
    "prettier.embeddedLanguageFormatting": "auto",
    "prettier.singleAttributePerLine": true,
    "[markdown]": {
        "editor.formatOnSave": false
    },
    "explorer.confirmDragAndDrop": true,
    "explorer.openEditors.visible": 1,
    "extensions.autoUpdate": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.suggestSmartCommit": false,
    "explorer.compactFolders": false,
    "terminal.integrated.copyOnSelection": true,
    "security.workspace.trust.untrustedFiles": "open",
    "diffEditor.ignoreTrimWhitespace": false,
    "html.format.contentUnformatted": "pre,code,textarea",
    "html.format.enable": true,
    "csscomb.syntaxAssociations": {
        "postcss": "scss"
    },
    "csscomb.formatOnSave": true,
    "csscomb.ignoreFilesOnSave": [],
    "csscomb.preset": "~/.vscode/csscomb.json",
    "liveSassCompile.settings.compileOnWatch": false,
    "liveSassCompile.settings.generateMap": false,
    "liveSassCompile.settings.excludeList": ["/**/node_modules/**", "/.vscode/**", "/.history/**"],
    "typescript.updateImportsOnFileMove.enabled": "always",
    "python.defaultInterpreterPath": "python3",
    "errorLens.excludeBySource": ["intelephense(1010)", "ts(7026)"],
    "redhat.telemetry.enabled": true,
    "intelephense.stubs": [
        "apache",
        "bcmath",
        "bz2",
        "calendar",
